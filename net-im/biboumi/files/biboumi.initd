pidfile="/var/run/biboumi.pid"
command="/usr/bin/biboumi"
command_args="${BIBOUMI_CONFIG:-/etc/biboumi/biboumi.cfg}"
command_user="${BIBOUMI_USER:-jabber}"
command_background="true"

depend() {
	use net
	use jabber-server
}
